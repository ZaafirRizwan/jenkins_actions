[MESSAGES CONTROL]
disable = C0114, C0115, C0116, C0103, W0511, R0913

[FORMAT]
max-line-length=120

[DESIGN]
max-args=10
max-locals=25
max-returns=10

[VARIABLES]
init-import=no

[TYPECHECK]
ignored-modules=typing

[SIMILARITIES]
min-similarity-lines=3
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no
